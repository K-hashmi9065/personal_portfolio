{
  "buildCommand": "bash build.sh",
  "outputDirectory": "build/web",
  "devCommand": "flutter run -d web-server --web-hostname=0.0.0.0 --web-port=3000",
  "framework": null,
  "cleanUrls": true,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
